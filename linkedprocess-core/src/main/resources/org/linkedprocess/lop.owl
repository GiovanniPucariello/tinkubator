<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl2 "http://www.w3.org/2006/12/owl2#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://linkedprocess.org/2006/06/lop.owl#"
     xml:base="http://linkedprocess.org/2006/06/lop.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl2="http://www.w3.org/2006/12/owl2#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <rdfs:comment xml:lang="en">Linked Process</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkedprocess.org/2006/06/lop.owl#spawned -->

    <owl:ObjectProperty rdf:about="#spawned">
        <rdfs:domain rdf:resource="#Farm"/>
        <rdfs:range rdf:resource="#VirtualMachine"/>
    </owl:ObjectProperty>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#spawnedBy -->

    <owl:ObjectProperty rdf:about="#spawnedBy">
        <rdfs:range rdf:resource="#Farm"/>
        <rdfs:domain rdf:resource="#VirtualMachine"/>
        <owl:inverseOf rdf:resource="#spawned"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkedprocess.org/2006/06/lop.owl#jid -->

    <owl:DatatypeProperty rdf:about="#jid">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#XmppClient"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#vmEngine -->

    <owl:DatatypeProperty rdf:about="#vmEngine">
        <rdfs:domain rdf:resource="#VirtualMachine"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#vmEngineVersion -->

    <owl:DatatypeProperty rdf:about="#vmEngineVersion">
        <rdfs:domain rdf:resource="#VirtualMachine"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#vmLanguage -->

    <owl:DatatypeProperty rdf:about="#vmLanguage">
        <rdfs:domain rdf:resource="#VirtualMachine"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#vmLanguageVersion -->

    <owl:DatatypeProperty rdf:about="#vmLanguageVersion">
        <rdfs:domain rdf:resource="#VirtualMachine"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#vmSpecies -->

    <owl:DatatypeProperty rdf:about="#vmSpecies">
        <rdfs:domain rdf:resource="#VirtualMachine"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkedprocess.org/2006/06/lop.owl#Farm -->

    <owl:Class rdf:about="#Farm">
        <rdfs:subClassOf rdf:resource="#XmppClient"/>
    </owl:Class>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#VirtualMachine -->

    <owl:Class rdf:about="#VirtualMachine">
        <rdfs:subClassOf rdf:resource="#XmppClient"/>
    </owl:Class>
    


    <!-- http://linkedprocess.org/2006/06/lop.owl#XmppClient -->

    <owl:Class rdf:about="#XmppClient">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
</rdf:RDF>
